<!doctype html>
<html>
	<head>
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<meta name="viewport" content="width=1408, initial-scale=1.0" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<link
			rel="stylesheet"
			href="/library/bootstrap.min.css"
			type="text/css"
			media="all"
		/>
		<link rel="stylesheet" href="/css/style.css" type="text/css" media="all" />
		<link rel="stylesheet" href="/css/artist.css" type="text/css" media="all" />
		<title>Artist</title>
		<style>
		  .top-artist-section{
		    background-image: url("/img/artists-cover/<%= artist.cover %>");
		  }
		</style>
	</head>
	<body>
		<div class="grid">
			<%- include('./partials/navbar.ejs') -%>
			<div class="section">
				<div class="artist-left">
					<div class="artist-profile-section">
						<div class="top-artist-section">
						  <div class="top-artist-contents">
							  <div class="artists-photo-container">
								<img
									class="artist-photo"
									src="/img/Artists/<%= artist.photo %>"
									alt=""
								/>
								<p class="artist-name"><%= artist.name %></p>
								<em class="bio"><%= artist.bio %></em>
							</div>
							  
						  </div>
						  <div class="contact-artist">
								<img class="square-insta" src="/assets/yutub.png" alt="" />
								<img class="" src="/assets/facebook.png" alt="" />
								<img class="square-insta" src="/assets/instagram.png" alt="" />
								<img class="" src="/assets/spotify.png" alt="" />
							</div>
						</div>
					</div>
					<div class="discography">
							<div class="overview ">
								<div>
								  <span>
								    <%= songs %>
								  </span> Song </div>
								<div>
								  <span class="">
								    <%= albums.length %>
								  </span> Album
								</div>
								<div>
								  <span>
								    <%= artist.totalSingles %>
								  </span> Single
								</div>
							</div>
					  
						<div class="albums-section">
							<h2 class="discography-title art-page-title">Albums</h2>
							<div class="discs" id="Albums-Div">
							<% if(albums.length > 0) {  %>
							  <% albums.forEach(album => { %>
  								<div class="disc">
  									<img
  										class="disc-art"
  										src="/img/Albums/<%= album.art %>"
  										alt=""
  										data-bs-toggle="modal"
  										data-bs-target="#exampleModal"
  									/>
  									<p class="disc-name" id="album-name-id"><%= album.name %></p>
  									<p class="disc-artist"><%= artist.name %></p>
  								</div>
							  <% }) %>
							<% } else { %>
								<div class="disc err-disc" id="eee">
									<img
										class="disc-art"
										src="/img/404.jpg"
										alt=""
									/>
									<p class="disc-name">No Albums Yet ...</p>
									<p class="disc-artist"></p>
								</div>
							<% } %>
								
							</div>
						</div>
						<div class="singles-section">
							<h2 class="discography-title art-page-title">
								Singles & Video Clips
							</h2>
							<div class="discs" id="singles">
							<% if(singles.length > 0) {  %>
							  <% singles.forEach(single => { %>
  								<div class="disc" id="Singles-Div">
  									<img
  										class="disc-art"
  										src="/img/Singles/<%= single.art %>"
  										alt=""
  										data-bs-toggle="modal"
  										data-bs-target="#exampleModal"
  									/>
  									<p class="disc-name" id="album-name-id"><%= single.name %></p>
  									<p class="disc-artist"><%= single.artist.name %>
  									  <% if(single.feat != "") { %>
  									    <%= `ft. ${single.feat}` %>
  									  <% } %>
  									</p>
  								</div>
							  <% }) %>
							<% } else { %>
								<div class="disc err-disc">
									<img
										class="disc-art"
										src="/img/404.jpg"
										alt=""
									/>
									<p class="disc-name">No Albums Yet ...</p>
									<p class="disc-artist"></p>
								</div>
							<% } %>
								
							</div>
							
						</div>
					</div>
				</div>
				<div class="biography">
					<h2 class="art-page-title">Biography</h2>
					<div class="biography-text-container">
						<p class="biography-text">
							Lorem ipsum dolor sit amet, consectetur adipisicing elit. Facere
