					</div>
				</div>
			</div>
			<%- include('./partials/footer.ejs') -%>
		</div>
		
		<!-- Modal -->
		<div
			class="modal fade"
			id="exampleModal"
			tabindex="-1"
			aria-labelledby="exampleModalLabel"
		>
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<h1 class="modal-title fs-5" id="exampleModalLabel">Sanqa Souq EP</h1>
						<button
							type="button"
							class="btn-close"
							data-bs-dismiss="modal"
							aria-label="Close"
						></button>
					</div>
					<div class="modal-body">
						<div class="paget">
							<div id="modal-img-cont">
								<img
									class="car-img"
									src="/img/Albums/sanqa suq.jpg"
									alt=""
									id="modal-img"
								/>
							</div>
							<ul class="modal-details">
								<li>
									Album Name:
									<span class="alb-name-span">...</span>
								</li>
								<li>Album Artist: <span class="artist-name-span">...</span></li>
								<li>Featuring: <span class="feat-name-span">...</span></li>
								<li>Producer <span class="producer-name-span">...</span></li>
								<li id="tt">
									Total Tracks: <span class="tracks-span">...</span>
								</li>
								<li>Released Date: <span class="released-span">...</span></li>
							</ul>
							<p class="ttill-it-load"></p>
							<div class="panel-group" id="accordion">
								<div class="panel panel-default">
									<div class="panel-heading">
										<p class="panel-title">
											<a
												data-bs-toggle="collapse"
												data-bs-parent="#accordion"
												href="#collapseOne"
											>
												More Information ...
											</a>
										</p>
									</div>
									<div id="collapseOne" class="panel-collapse collapse in">
										<div class="panel-body till-it-load">
											<p class="details-text"></p>
											<a href="/album-details/Asalafi"> See More... </a>
											<br /><br />
											<!-- <a class="text-center" data-bs-toggle="collapse" data-bs-parent="#accordion" href="#collapseOne">
          Show Less...
         </a> -->
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>

					<div class="modal-footer">
						<div class="get-album">
								<img class="" src="/assets/yutub.png" alt="" />
								<img class="" src="/assets/spotify.png" alt="" />
							<img class="" src="/assets/amazon music.png" alt="" />
								<img class="square-insta" src="/assets/instagram.png" alt="" />
						</div>
						<div id="button">
							<button
								type="button"
								class="btn btn-warning"
								data-bs-dismiss="modal"
							>
								Close It
							</button>
						</div>
					</div>
				</div>
			</div>
		</div>

		
		
		
		<script src="/js/index.js" type="text/javascript" charset="utf-8"></script>
